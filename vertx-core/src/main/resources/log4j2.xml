<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%blue{%d{HH:mm:ss.SSS}} [%t] %green{%-5level} %magenta{%c{1.}}: %green{%msg}%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Logger name="com.ebento" level="DEBUG" />
    <Root level="INFO">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>
